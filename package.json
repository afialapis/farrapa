{
  "name": "farrapa",
  "private": true,
  "description": "diverse utils used by afialapis",
  "author": "Donato Lorenzo <donato@afialapis.com>",
  "contributors": [
    "Donato Lorenzo <donato@afialapis.com>"
  ],
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/afialapis/farrapa.git"
  },
  "bugs": {
    "url": "https://github.com/afialapis/farrapa/issues"
  },
  "homepage": "https://farrapa.afialapis.com/",
  "scripts": {
    "lint": "./script.sh lint",
    "clean-lib": "./script.sh clean-lib",
    "lib": "npm run clean-lib && ./script.sh lib",
    "versioning": "./script.sh version",
    "clean-dist": "./script.sh clean-dist",
    "dist": "npm run clean-dist && ./script.sh dist",
    "clean": "npm run clean-lib && npm run clean-dist",
    "reset": "npm run clean && pnpm i",
    "site": "rm -fr arredemo && npx xeira site",
    "prepare": "npm run clean && npm run lint && npm run dist && npm run site",
    "all:publish": "./script.sh publish"
  },
  "keywords": [
    "js",
    "util"
  ],
  "devDependencies": {
    "xeira": "0.1.0-beta.3"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/xeira/configs/eslint.cjs"
    ]
  }
}
